#!/bin/bash

# Path
export PATH="/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin"

# Android
export ANDROID_HOME=/Users/carloseduardo/Library/Android/sdk
export ANDROID_TOOLS=$ANDROID_HOME/tools
export ANDROID_BUILD_TOOLS=$ANDROID_HOME/build-tools/22.0.1
export ANDROID_PLATFORM_TOOLS=$ANDROID_HOME/platform-tools

path+=(“$ANDROID_HOME”, "$ANDROID_TOOLS", "$ANDROID_BUILD_TOOLS", "$ANDROID_PLATFORM_TOOLS")

# Java
export JAVA_HOME=/Library/Java/JavaVirtualMachines/jdk1.7.0_79.jdk/Contents/Home
export JAVA7_HOME=/Library/Java/JavaVirtualMachines/jdk1.7.0_79.jdk/Contents/Home
export JAVA8_HOME=/Library/Java/JavaVirtualMachines/jdk1.8.0_45.jdk/Contents/Home
path+=(“$JAVA_HOME”, “$JAVA7_HOME”, “$JAVA8_HOME”)

# Kotlin
export KOTLIN_HOME=/Users/carloseduardo/compiler/kotlinc/bin
path+=("$KOTLIN_HOME")


# Code Inspector
export INFER=/Users/carloseduardo/Documents/Projetos/infer-osx-v0.3.0/infer/bin
path+=("$INFER")

# Ruby
export RVM=$HOME/.rvm/bin
path+=("$RVM")

# MySQL
export MYSQL=/usr/local/mysql/bin
path+=("$MYSQL")

# Assembler - Disassembler
export DEXDECODE=$HOME/Documents/assembler_disassembler
path+=("$DEXDECODE")

# CMake
export CMAKE=/Applications/CMake.app/Contents/bin
path+=("$CMAKE")

# MacPorts
export MACPORTS=/opt/local/bin
path+=("$MACPORTS")

# Z
export Z=/usr/local/etc/profile.d
