# MacPorts
export MACPORTS="/opt/local/bin"
path+=("$MACPORTS")

# Z
export Z="/usr/local/etc/profile.d"
path+=("$Z")

# Genymotion
export PLAYER="/Applications/Genymotion.app/Contents/MacOS/player.app/Contents/MacOS/"
path+=("$PLAYER")

# Sonar
export SONAR="$HOME/sonarqube"
